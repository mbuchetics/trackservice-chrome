{
	"name": "FM4 Spotify",
	"version": "0.1",
	"description": "FM4 Trackservice meets Spotify",
	"icons": { 
		"16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png" 
	},
	"homepage_url": "http://trackservice.herokuapp.com",
	"background_page": "background.html",
	"permissions": [
		"tabs",
		"notifications",
		"http://trackservice.herokuapp.com/api/*",
		"*.facebook.com/*"
	],
	"browser_action": {
		"default_icon": "images/icon19.png",
		"default_title": "FM4 trackservice",
		"default_popup": "popup.html"
	}
}